  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "discord.js": "^11.3.2",
    "discord.js-commando": "^0.9.0",
    "express": "^4.16.3",
    "ffmpeg-binaries": "^3.2.2-3",
    "google-images": "^2.1.0",
    "jsonfile": "^4.0.0",
    "material-ui": "^0.20.0",
    "node-opus": "^0.2.7",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
