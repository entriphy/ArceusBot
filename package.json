    "babel-preset-stage-1": "^6.24.1",
    "discord.js": "^11.1.0",
    "express": "^4.15.5",
    "google-images": "^2.1.0",
    "jsonfile": "^4.0.0",
    "material-ui": "^0.19.2",
    "node-opus": "^0.2.6",
