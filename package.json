{
  "name": "arceusbot",
  "version": "1.0.0",
  "description": "A bot for Discord.",
  "main": "index.js",
  "scripts": {
    "start:bot": "node index.js",
    "start:dev": "cross-env NODE_ENV=development concurrently \"node server.js\" \"npm run start:bot\"",
    "start": "cross-env NODE_ENV=production concurrently \"webpack --config webpack.prod.config.js && node server.js\" \"npm run start:bot\""
  },
