    "babel-preset-stage-1": "^6.24.1",
    "discord.js": "^11.2.1",
    "express": "^4.16.2",
    "google-images": "^2.1.0",
    "jsonfile": "^4.0.0",
    "material-ui": "^0.20.0",
    "opusscript": "^0.0.6",
